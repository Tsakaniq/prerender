# Prerender Service Configuration
# No TARGET_URL needed - Host header from ALB is used for domain detection

# Optional: Server port (default: 3000)
PORT=3000

# Optional: Environment (default: development)
NODE_ENV=production

# Optional: Customize cache TTL (in seconds)
CACHE_TTL=3600

# Optional: Customize browser timeout (in milliseconds)
BROWSER_TIMEOUT=30000